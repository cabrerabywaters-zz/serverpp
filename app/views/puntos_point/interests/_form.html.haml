= simple_form_for [:puntos_point, @interest], html: {class: 'form-horizontal' } do |f|
  = f.input :name
  .form-actions
    = f.button :submit, class: 'btn-warning pull-right ml-10'
    - if can? :read, Interest
      = link_to t("helpers.links.cancel"), puntos_point_interests_path, class: 'btn btn-inverse pull-right'
