/* Less
------------------------------------------------------------------*/

/* Font */
.defaultFont {font-family: 'Open Sans', Helvetica, Verdana, Geneva, sans-serif;}
.carouselFont {font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;}

/* Colors */
@defaultBackground:@secondColor;
@firstColor: #51565b;
@secondColor:black;
@thirdColor:black;
@fourthColor:#f7a800;
@white:#fff;
@black:#000;
@futbol:#006699;
.white-text{color:@white !important; }
.black-text{color: @black !important;}
.green-text{color: #387038 !important;}


/* Backgrounds */
.background{
	background: #ededed;
}
.backgroundHeader, .backgroundFooter, .boxCanje{
	background: rgba(0, 0, 0, 0.88);

}
.backgroundHeader{
	border-bottom: 2px;
font-size: 1.03em;
position: fixed !important;
top: 0px !important;
z-index: 9999;
width: 100%;
}
.background-transparencia-negra{background:url(<%= asset_path "bg_negro.png" %>) repeat scroll 0px 0px transparent;}
.background-transparencia-negra div i{color: white;}
/* Radius */
.defaultRadius (@radius:6px) {
	-webkit-border-radius:@radius;
	-moz-border-radius:@radius;
	border-radius:@radius;
}



/* Generals
------------------------------------------------------------------*/
html, body, form, fieldset, h1, h2, h3, h4, h5, h6, p, pre, ul, ol, dl, hr {cursor:default;text-align: center;color:black;}
html, body, div, span, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
abbr, address, cite, code,
del, dfn, em, img, ins, kbd, q, samp,
small, strong, sub, sup, var,
b, i,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figcaption, figure,
footer, header, hgroup, menu, nav, section, summary,
time, mark, audio, video {
    outline:0;
    background:transparent;
  
}
html {
  overflow-y: scroll;
}
html, body {height: 100%;}
body {
